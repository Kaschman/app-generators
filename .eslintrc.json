{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": ["error", { "args": "after-used" }],
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": ["src/api/templates/_src/*"],
      "rules": {
        "import/no-unresolved": "off",
        "no-console": "off"
      }
    }
  ]
}
